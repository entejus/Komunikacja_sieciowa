<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.student.komunikacja_sieciowa.MainActivity">

    <TextView
        android:id="@+id/czas_dzialania_etykieta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Czas dzialania:"
        />
    <TextView
        android:id="@+id/wynik_etykieta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="wynik:"
        />
    <Button
        android:id="@+id/uruchom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Uruchom"
        />

</LinearLayout>